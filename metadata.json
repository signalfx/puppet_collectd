{
  "name": "signalfx-collectd",
  "version": "0.1.13",
  "author": "Uday Sagar Shiramshetty",
  "summary": "A puppet module that configures complete collectd from SignalFx",
  "license": "Apache-2.0",
  "source": "https://github.com/signalfx/puppet_collectd",
  "project_page": "https://github.com/signalfx/puppet_collectd",
  "issues_url": "https://github.com/signalfx/puppet_collectd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2017.09",
        "2017.03",
        "2016.09",
        "2016.03",
      	"2015.09",
        "2015.03",
        "2014.09"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RHEL",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.5.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.0.4"},
    {"name":"puppetlabs/apt","version_requirement":">= 2.2.2"},
    {"name":"puppetlabs/git","version_requirement":">= 0.4.0"},
    {"name":"puppetlabs/vcsrepo","version_requirement":">=1.0.2"}
  ]
}
